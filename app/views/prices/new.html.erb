<h1>New Price</h1>

<%= form_for :price, url: prices_path do |f| %>
  <p>
    <%= f.label :station_fuel_type_id %><br>
    <%= f.text_field :station_fuel_type_id %>
  </p>
 
  <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>
 
  <p>
    <%= f.label :time %><br>
    <%= f.text_field :time %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', prices_path %>
