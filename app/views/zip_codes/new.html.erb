<h1>New Zip Code</h1>

<%= form_for :zip_code, url: zip_codes_path do |f| %>
  <p>
    <%= f.label :zip_code %><br>
    <%= f.text_field :zip_code %>
  </p>
 
  <p>
    <%= f.label :lat %><br>
    <%= f.text_field :lat %>
  </p>
 
  <p>
    <%= f.label :lon %><br>
    <%= f.text_field :lon %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', zip_codes_path %>
