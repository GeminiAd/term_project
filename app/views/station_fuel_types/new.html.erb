<h1>New Station Fuel Type</h1>

<%= form_for :station_fuel_type, url: station_fuel_types_path do |f| %>
  <p>
    <%= f.label :station_id %><br>
    <%= f.text_field :station_id %>
  </p>
 
  <p>
    <%= f.label :fuel_type_id %><br>
    <%= f.text_field :fuel_type_id %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', station_fuel_types_path %>
