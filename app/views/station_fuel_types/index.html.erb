<h1>Listing Station Fuel Types</h1>
 
<%= link_to 'New Station Fuel Type', new_station_fuel_type_path %>
<table border="1">
  <tr>
    <th>Id</th>
    <th>Station Id</th>
    <th>Fuel Type Id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
  <% @station_fuel_types.each do |station_fuel_type| %>
    <tr>
      <td><%= station_fuel_type.id %></td>
      <td><%= station_fuel_type.station_id %></td>
      <td><%= station_fuel_type.fuel_type_id %></td>
      <td><%= link_to 'Show', station_fuel_type %></td>
      <td><%= link_to 'Edit', edit_station_fuel_type_path(station_fuel_type) %></td>
      <td><%= link_to 'Destroy', station_fuel_type_path(station_fuel_type),
                      method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<br />

<%= link_to 'Back', controller: "welcome" %>
