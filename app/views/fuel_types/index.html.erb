<h1>Listing Fuel Types</h1>
 
<%= link_to 'New Fuel Type', new_fuel_type_path %>
<table border="1">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Description</th>
    <th>URL</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
  <% @fuel_types.each do |fuel_type| %>
    <tr>
      <td><%= fuel_type.id %></td>
      <td><%= fuel_type.name %></td>
      <td><%= fuel_type.description %></td>
      <td><%= fuel_type.url %></td>
      <td><%= link_to 'Show', fuel_type %></td>
      <td><%= link_to 'Edit', edit_fuel_type_path(fuel_type) %></td>
      <td><%= link_to 'Destroy', fuel_type_path(fuel_type),
                      method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<br />

<%= link_to 'Back', controller: "welcome" %>
