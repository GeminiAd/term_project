<h1>New City</h1>

<%= form_for :city, url: cities_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :state %><br>
    <%= f.text_field :state %>
  </p>
 
  <p>
    <%= f.label :country %><br>
    <%= f.text_field :country %>
  </p>
 
  <p>
    <%= f.label :lat %><br>
    <%= f.text_field :lat %>
  </p>
 
  <p>
    <%= f.label :lon %><br>
    <%= f.text_field :lon %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', cities_path %>
